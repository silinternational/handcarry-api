create_table("images") {
	t.Column("id", "integer", {primary: true})
	t.Column("uuid", "uuid", {})
	t.Column("url", "string", {null: true, "size": 1024})
	t.Column("url_expiration", "timestamp")
	t.Column("post_id", "integer")
	t.Timestamps()
    t.Index("uuid", {"unique": true})
	t.ForeignKey("post_id", {"posts": ["id"]}, {"on_delete": "cascade"})
}
